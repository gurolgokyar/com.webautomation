<?xml version="1.0" encoding="UTF-8"?>
<suite name = "datepicker" parallel = "classes" thread-count = "2">
    <listeners>
        <listener class-name = "listeners.ExtentReportListener"/>
    </listeners>
    <test name = "testWithChrome">
        <parameter name = "browser" value = "chrome" />
        <classes>
            <class name = "datepicker.DisplayMultipleMonthsPageTest" />
            <class name = "datepicker.SelectDateRangePageTest"/>
        </classes>
    </test>
</suite>