<suite name="controlGroup" parallel="classes" thread-count="2">
    <listeners>
        <listener class-name = "listeners.ExtentReportListener"/>
    </listeners>

    <test name="testingWithChrome">
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="controlGroupTest.ToolBarPageTest"/>
        </classes>
    </test>

    <test name="testingWithEdge">
        <parameter name="browser" value="edge"/>
        <classes>
            <class name="controlGroupTest.ToolBarPageTest"/>
        </classes>
    </test>
</suite>